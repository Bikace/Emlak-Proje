        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsinmaSekli)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Otopark)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Adre.Il)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DaireTip.Adi)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.KonutDurum.Adi)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EmlakID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EmlakID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EmlakID })
        </td>
    </tr>
}

</table>
