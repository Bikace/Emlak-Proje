@model IEnumerable<ES.Data.Emlak>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Emlak oluştur", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KatNumarasi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DaireNo)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.KiraBedeli)
