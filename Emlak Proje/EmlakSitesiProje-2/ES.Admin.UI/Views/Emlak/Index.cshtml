            @Html.DisplayNameFor(model => model.DaireTip.Adi)
        </th>
 
        <th>
            @Html.DisplayNameFor(model => model.KonutDurum.Adi)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KatNumarasi)
        </td>
        <td>
